$white: rgba(255, 255, 250, 1);
$black: rgba(0, 0, 0, 1);

$green-highlight: rgba(63, 241, 170, 1);
$green-highlight-light: lighten($green-highlight, 20%);

$faded-green: rgba(233, 247, 240, 1);
$base-color-lightest: rgba(120, 205, 215, 1);
$base-color-lighter: rgba(68, 161, 160, 1);
$base-color: rgba(10, 126, 139, 1);
$darker-green: rgba(13, 92, 99, 1);
$darkest-green: rgb(4, 29, 38);

$green-highlight-o80: rgba(63, 241, 170, 0.8);
$base-color-lightest-o80: rgba(120, 205, 215, 0.8);
$base-color-lighter-o80: rgba(68, 161, 160, 0.8);
$base-color-o80: rgba(10, 126, 139, 0.8);
$darker-green-o80: rgba(13, 92, 99, 0.8);

$grey: rgba(51, 51, 51, 1);


$m1: 10px;
$m2: 20px;
$m3: 40px;
$m4: 80px;
$m5: 160px;


$max-width-full: 1100px;


@mixin gradient-base-to-darker-green {
  /* Permalink - use to edit and share this gradient: https://colorzilla.com/gradient-editor/#3ff1aa+0,0d5c63+100 */
  background: $green-highlight;
  background: -moz-linear-gradient(45deg,  $green-highlight 0%, $darker-green 100%);
  background: -webkit-linear-gradient(45deg,  $green-highlight 0%,$darker-green 100%);
  background: linear-gradient(45deg,  $green-highlight 0%,$darker-green 100%);
}

@mixin gradient-base-to-darker-green-o80 {
  /* Permalink - use to edit and share this gradient: https://colorzilla.com/gradient-editor/#3ff1aa+0,0d5c63+100 */
  background: $green-highlight-o80;
  background: -moz-linear-gradient(45deg,  $green-highlight-o80 0%, $darker-green-o80 100%);
  background: -webkit-linear-gradient(45deg,  $green-highlight-o80 0%,$darker-green-o80 100%);
  background: linear-gradient(45deg,  $green-highlight-o80 0%,$darker-green-o80 100%);
}

@mixin gradient-base-to-darker-green--short {
  /* Permalink - use to edit and share this gradient: https://colorzilla.com/gradient-editor/#3ff1aa+0,0d5c63+100 */
  background: $green-highlight;
  background: -moz-linear-gradient(45deg,  $green-highlight 20%, $darker-green 100%);
  background: -webkit-linear-gradient(45deg,  $green-highlight 20%,$darker-green 100%);
  background: linear-gradient(45deg,  $green-highlight 20%,$darker-green 100%);
}

@mixin gradient-test {
  /* Permalink - use to edit and share this gradient: https://colorzilla.com/gradient-editor/#0a1d33+0,1ae091+100 */
  background: rgb(10,29,51); /* Old browsers */
  background: -moz-linear-gradient(45deg,  rgba(10,29,51,1) 0%, rgba(26,224,145,1) 100%);
  background: -webkit-linear-gradient(45deg,  rgba(10,29,51,1) 0%,rgba(26,224,145,1) 100%);
  background: linear-gradient(45deg,  rgba(10,29,51,1) 0%,rgba(26,224,145,1) 100%);
}

@mixin mlra {
  margin-left: auto;
  margin-right: auto;
}




div:before, ul:before, article:before, aside:before, details:before, figcaption:before, figure:before, footer:before, header:before, hgroup:before, menu:before, nav:before, section:before, div:after, ul:after, article:after, aside:after, details:after, figcaption:after, figure:after, footer:after, header:after, hgroup:after, menu:after, nav:after, section:after {
  content: none;
}





// Container for whole top header block
#colibri .h-navigation_outer.style-local-16-h2-outer {
  display: flex;
  flex-flow: column nowrap;
}

// Logo & Navigation block
#colibri .style-2 {
  order: 1;
  padding: 0 $m2;
  background-color: rgba(255, 255, 255, .3);
}

// Outer container element for all the top menu items, social media, 
#colibri .h-navigation_outer.style-local-16-h2-outer > .h-section-grid-container.h-section-fluid-container {
  order: 2;
}


// Green gradient button
#colibri a.style-31 {
  position: relative;
  background: transparent;
  z-index: 10;
  text-shadow: -1px 1px $darker-green;
}

// Green gradient button:before for background-gradient transition
#colibri a.style-31::before {
  @include gradient-base-to-darker-green();
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: -1;
  opacity: 1;
  border-radius: 300px;
  transition: all .5s;
}

// Override styles from parent theme for green button hover, etc
#colibri a.style-31:hover,
#colibri a.style-31:focus,
#colibri a.style-31:active {
  background: transparent;
}

// Transition opacity on hover, etc
#colibri a.style-31:hover::before,
#colibri a.style-31:focus::before,
#colibri a.style-31:active::before {
  opacity: 0.8;
}

// Contact icons at top of page
#colibri .style-22-icon {
  fill: $green-highlight-light;
}

// text of the contact links at top of page
#colibri .style-22 .list-text {
  transition: all .5s;
}

// hover, etc for text of contact links at top of page
#colibri .style-22 .list-text:hover,
#colibri .style-22 .list-text:focus,
#colibri .style-22 .list-text:active {
  color: $green-highlight-light;
}

// Logo
#colibri .style-5 .logo-text {
  transition: all .5s;
  text-transform: uppercase;
}

// Logo hover, etc
#colibri .style-5 .logo-text:hover,
#colibri .style-5 .logo-text:focus,
#colibri .style-5 .logo-text:active {
  color: $green-highlight-light;
}

// Hover, etc for social media icons
#colibri .style-24 .icon-container:hover,
#colibri .style-24 .icon-container:focus,
#colibri .style-24 .icon-container:active {
  fill: $green-highlight-light;
}

// Primary navigation at top of page hover, etc
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu > li > a:hover,
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu > li > a:focus,
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu > li > a:active {
  color: $green-highlight-light;
}

// Primary navigation of current page item while at tops of page hover, etc
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a:hover,
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a:focus,
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a:active {
  color: $green-highlight-light;
}

// Remove underline effects of main nav items
#colibri .style-9 > div > .colibri-menu-container > ul.colibri-menu.bordered-active-item > li::after, #colibri .style-9 ul.colibri-menu.bordered-active-item > li::before {
  background-color: transparent;
}

// Removing underline of active nav link while page scrolled
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu.bordered-active-item > li::after,
#colibri .h-navigation_sticky .style-9 ul.colibri-menu.bordered-active-item > li::before,
#colibri .h-navigation_sticky.style-9 > div > .colibri-menu-container > ul.colibri-menu.bordered-active-item > li::after,
#colibri .h-navigation_sticky.style-9 ul.colibri-menu.bordered-active-item > li::before {
  color: $black;
  background-color: transparent;
}

// Adjusting color of active nav link
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a {
  color: $black;
}

// Hover effect for logo while page scrolled
#colibri .h-navigation_sticky .style-5 .logo-text:hover,
#colibri .h-navigation_sticky .style-5 .logo-text:focus,
#colibri .h-navigation_sticky .style-5 .logo-text:active {
  color: $green-highlight;
}

// Hover, etc effect for nav links while page scrolled
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li:hover > a,
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li:focus > a,
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li:active > a,
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a:hover,
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a:focus
#colibri .h-navigation_sticky .style-9 > div > .colibri-menu-container > ul.colibri-menu > li.current_page_item > a:active {
  color: $green-highlight;
}

// first child of the top contact info bar
#colibri .style-20 {
  padding: $m1 0 0;
  background-color: rgba(255, 255, 255, .2);
  background-color: transparent;
}

// grandchild of top contact info bar
#colibri .style-20 .h-section-boxed-container {
  @include mlra();
  background: transparent;
  max-width: $max-width-full;
}

// Prevent top contact bar from being hidden on mobile
.h-hide-sm {
  display: block;
}

// Inside-most container of the contact info and social media blocks
#colibri .h-hide-sm .h-row {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between !important;
  width: 100%;
  padding: $m1 $m2;
}

// Contact info block
#colibri .h-hide-sm .style-local-16-h21-outer {
  flex: 1 1 auto;
}

// list items within contact info block
#colibri .h-hide-sm .style-local-16-h21-outer .list-item {
  margin-bottom: 2px;
  flex: 0 0 auto;
}
#colibri .h-hide-sm .style-local-16-h21-outer .list-item:last-of-type {
  margin-bottom: 0;
}

// ul within the contact info block
.ul-list-icon.horizontal-on-mobile {
  display: block;
}

// li within the contact info block
.ul-list-icon.horizontal-on-mobile .list-item {
  display: block;
}

// max-width on small screens so social media icons display 2x2
#colibri .h-hide-sm .style-local-16-h23-outer {
  max-width: 80px;
  width: unset;
}

// increase size of icons
#colibri .style-24 .icon-container {
  width: 22px;
  height: 22px;
}

// shrink space between logo and menu icon so logo 1 line long only
#colibri .style-local-16-h6-outer {
  max-width: 0;
}

#colibri .style-4-outer.style-local-16-h4-outer {
  padding-left: 0;
}

// Menu Icon outer container
#colibri .style-local-16-h8-outer {
  transform: translateX(15px);
}

// Social Icons Container
.h-social-icons {
  transform: translateX(5px);
  justify-content: flex-end !important;
}

// Adjust the linkedin and github social icon positions
#colibri .style-24 .social-icon-container:nth-last-of-type(1),
#colibri .style-24 .social-icon-container:nth-last-of-type(2) {
  padding-top: 2px;
}

// Hero video section
#colibri #hero {
  padding-left: $m2;
  padding-right: $m2;
}

// Main header in hero section
#colibri #hero h1 {
  font-size: 2em;
  line-height: 1.1;
  margin-bottom: $m3;
}

// Container for secondary header in hero section
#colibri #hero .style-local-16-h29 {
  margin-bottom: $m3;
}

// Secondary header in hero section
#colibri #hero .style-29 p {
  font-size: .7em;
  line-height: 1.3;
}

// Page section created by wp-calibri with spacers
#content .h-section-global-spacing {
  padding: 0;
}

#content .h-section-boxed-container {
  max-width: unset;
}

// Footer container
#colibri .jed-footer {
  color: $white;
  padding: $m2;
  background-color: $black;
  text-align: center;
}






// SECTION STYLES

// Fixing issue w/ wp-colibri sections causing horizontal scroll
#colibri .jed-section {
  padding: $m4 $m2;
  background-color: $white;
  text-align: left;
}

#colibri .content .h-row {
  margin: 0;
  display: block;
}
#colibri .h-col {
  padding: 0;
}

#colibri .jed-section__heading {
  font-weight: 900;
  margin-bottom: $m4;
  text-align: center;
}

#colibri .jed-section__subheading {
  margin: 0;
}

#colibri .jed-section--white__subheading {
  @include gradient-base-to-darker-green();
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
#colibri .jed-section--white__subheading--short {
  @include gradient-base-to-darker-green--short();
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

#colibri .jed-section__subheading4 {
  margin: 0;
  font-weight: 900;
  line-height: 1.1;
}

#colibri .jed-section--basecolor {
  background: $base-color-lighter;
  background-color: $darkest-green;
}

#colibri .jed-section--basecolor__heading {
  color: $white;
}

#colibri .jed-section--basecolor__subheading {
  color: $white;
  text-align: center;
  margin: $m4 0 $m3;
}

#colibri .jed-section--basecolor__subheading:first-of-type {
  margin: 0 0 $m3;
}

#colibri .jed-section--faded-green {
  background-color: $faded-green;
}

#colibri .jed-section--faded-green__heading {
  color: $grey;
}

#colibri .jed-section--faded-green__subheading {
  color: $black;
  text-align: center;
  margin: $m4 0 $m3;
}

#colibri .jed-section--faded-green__subheading:first-of-type {
  margin: 0 0 $m3;
}

#colibri .jed-section--dark-green {
  background-color: $darkest-green;
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/contact-background.jpg);
  background-size: cover;
}

#colibri .jed-section--dark-green__heading {
  color: $white;
}

#colibri .jed-section--dark-green__subheading {
  color: $white;
  text-align: center;
  margin: $m4 0 $m3;
}

#colibri .jed-section--dark-green__subheading:first-of-type {
  margin: 0 0 $m3;
}




// RESUME SECTION
#colibri .jed-resume-ctr {
  @include mlra();
  max-width: $max-width-full;
}

#colibri .jed-resume__row {
  margin-bottom: $m3;
}

#colibri .jed-resume__professional-summary {
  margin-top: $m2;
}

#colibri .jed-resume__subheading-ctr {
  display: flex;
  justify-content: flex-start;
}

#colibri .jed-resume__skills-subheading {
  margin-top: $m2;
}

#colibri .jed-resume__skills-ul {
  margin: 0;
}

#colibri .jed-resume__skills-li {
  display: inline-block;
  padding: 0 10px 0 15px;
  position: relative;
}

#colibri .jed-resume__skills-li:before {
  content: "\2022";
  position: absolute;
  top: 0;
  left: 4px;
  display: block;
  color: $base-color;
}

#colibri .jed-resume__name {
  font-style: italic;
  font-weight: 400;
  font-size: 1em;
  margin: $m2 0 0;
}

#colibri .jed-resume__job-details-ul {
  margin: 0;
}

#colibri .jed-resume__job-details-li {
  display: block;
  padding-left: 15px;
  position: relative;
}

#colibri .jed-resume__job-details-li:before {
  content: "\2022";
  position: absolute;
  top: 0;
  left: 4px;
  display: block;
  color: $base-color;
}





// PORTFOLIO SECTION

.jed-portfolio {
  max-width: $max-width-full;
  margin: 0 auto;
}

#colibri .jed-cards-ctr {
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  max-width: 940px;
  margin: 0 auto;
}

#colibri .jed-card {
  width: 100%;
  background-color: $white;
  margin-bottom: $m3;
  border-radius: 2%;
  max-width: 450px;
  display: flex;
  flex-flow: column nowrap;
  overflow: hidden;
  box-shadow: 0 0 12px rgba($darkest-green, .12);
}
#colibri .jed-card:nth-last-child(1) {
  margin-bottom: 0;
}

#colibri .jed-card__title {
  padding: $m1 $m2;
  position: relative;
  z-index: 999999;
  background: $white;
}

#colibri .jed-card__foot {
  padding: $m2;
  height: 100%;
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
}

#colibri .jed-card__content {
  background-repeat: no-repeat;
  background-size: cover;
  padding-bottom: 50%;
  position: relative;
  box-shadow: 0 0 12px rgba(0, 0, 0, .4);
  z-index: 1;
}

#colibri .jed-card__content--wordswift {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/wordswift-card-image.jpg);
}

#colibri .jed-card__content--lease-mileage {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/lease-mileage-card-image.jpg);
}

#colibri .jed-card__content--insurance {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/3d-insurance-card-image.jpg);
}
#colibri .jed-card__content--handy {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/handy-hendricks-card-image.jpg);
}
#colibri .jed-card__content--sc-laundromats {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/sc-laundromats-card-image.jpg);
}
#colibri .jed-card__content--owlworks {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/owlworks-card-image.jpg);
}

#colibri .jed-card__action-ctr {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}

#colibri .jed-card__action-ctr--a-only {
  justify-content: flex-end;
}

#colibri .jed-card__view-demo {
  color: $white;
  padding: 5px 14px;
  border: none;
  cursor: pointer;
  background: $grey;
  font-weight: bold;
  border-radius: 15px;
  transition: all 0.5s;
}

#colibri .jed-card__view-demo:hover,
#colibri .jed-card__view-demo:focus,
#colibri .jed-card__view-demo:active {
  background: $base-color;
}

#colibri .jed-card__go-to {
  color: $grey;
  font-weight: bold;
  transition: all 0.5s;
  position: relative;
  padding-left: 1.5em;
}

#colibri .jed-card__go-to:hover,
#colibri .jed-card__go-to:focus,
#colibri .jed-card__go-to:active {
  color: $base-color;
}

#colibri .jed-icon {
  background-size: 100% 100%;
  width: 1.3em;
  height: 1.3em;
  display: block;
  position: absolute;
  left: 0;
  top: 1px;
}
#colibri .jed-icon--github {
  // background-size: 100% 100%;
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/github-icon.png);
}
#colibri .jed-icon--external-a {
  background-image: url(https://www.joedisalvo.com/wp-content/uploads/external-a-icon.png);
}







// CONTACT ME SECTION

#colibri #jed-section-contact form {
  max-width: 500px;
  margin: 0 auto;
  padding: 40px;
  background: #ffffff22;
  background: rgba(255, 255, 255, .15);
  border-radius: 8px;
}

#colibri #jed-section-contact input,
#colibri #jed-section-contact textarea {
  width: 100%;
  border: none;
  padding: 10px;
  background: #ffffff33;
  background: rgba(255, 255, 255, .18);
  color: $white;
  border-radius: 6px;
}

#colibri #jed-section-contact label {
  color: $white;
}

#colibri .jed-contact-submit-ctr {
  margin: 0;
}

.google-captcha-msg {
  font-size: 10px;
  margin-bottom: 0;
  text-align: center;
}

.google-captcha-msg a {
  color: $green-highlight;
}







@media (min-width: 550px) {
  
  #colibri #navigation {
    padding: 0 $m3;
  }

  #colibri .h-hide-sm .h-row {
    padding: $m1 $m3;
  }

  #colibri #hero {
    padding-left: $m3;
    padding-right: $m3;
  }

  #colibri .jed-section {
    padding: $m4 $m3;
  }
}

@media (min-width: 576px) {
  
  .h-section-boxed-container {
    max-width: $max-width-full;
  }
}

@media (min-width: 645px) {

  // ul within the contact info block
  .ul-list-icon.horizontal-on-mobile {
    display: flex;
  }

  // li within the contact info block
  .ul-list-icon.horizontal-on-mobile .list-item {
    display: inline-flex;
  }

  // max-width on small screens so social media icons display 2x2
  #colibri .h-hide-sm .style-local-16-h23-outer {
    max-width: 100%;
  }

  #colibri .h-hide-sm .h-row {
    padding: 2px $m3;
  }

  // Main header in hero section
  #colibri #hero h1 {
    font-size: 3em;
    line-height: 1.1;
    margin-bottom: $m3;
  }

  // Container for secondary header in hero section
  #colibri #hero .style-local-16-h29 {
    margin-bottom: $m3;
  }

  // Secondary header in hero section
  #colibri #hero .style-29 p {
    font-size: 1em;
    line-height: 1.3;
  }
}

@media (min-width: 768px) {

  // Reverse shrinking of spacer between logo and menu icon/items on larger screens
  #colibri .style-local-16-h6-outer {
    max-width: 100%;
  }

  // Menu Icon outer container
  #colibri .style-local-16-h8-outer {
    transform: translateX(5px);
  }

  #colibri .jed-cards-ctr {
    justify-content: space-between;
  }

  #colibri .jed-card:nth-last-child(2) {
    margin-bottom: 0;
  }

  #colibri .jed-card {
    width: calc(50% - #{$m1});
  }
}

@media (min-width: 900px) {
  
  #colibri #navigation {
    padding: 0 $m4;
  }

  #colibri #hero {
    padding-left: $m4;
    padding-right: $m4;
  }

  #colibri .h-hide-sm .h-row {
    padding: 2px 0;
  }

  #colibri .style-20 {
    padding: $m1 $m4 0;
  }

  #colibri .jed-section {
    padding: $m4 $m4;
  }
}

@media (min-width: 1024px) {
  // Menu Icon outer container
  #colibri .style-local-16-h8-outer {
    transform: translateX(0);
  }
}




